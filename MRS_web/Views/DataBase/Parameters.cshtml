@using MRS_web.Models.EDM
@{
    ViewBag.Title = $"Список параметров счётчика \"{ViewData["MeterName"]}\"";
}

<h3>
    <a class="backButton" href="javascript:history.go(-1)">Назад</a>
</h3>

<h2>@ViewBag.Title</h2>

<table>
    <thead>
        <tr>
            <th>Название</th>
            <th>Значение</th>
        </tr>
    </thead>
    @{
        foreach (Parametr p in (IEnumerable<Parametr>) ViewData["Parameters"])
        {
            <tr>
                <td>@p.Name</td>
                <td>@p.Measure</td>
            </tr>
        }
    }
</table>

