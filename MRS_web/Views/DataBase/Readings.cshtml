@using MRS_web.Models.EDM
@{
    ViewBag.Title = $"Список показаний счётчика \"{ViewData["MeterName"]}\" ({ViewData["ReadingsUnit"]})";
}

<h3>
    <a class="backButton" href="javascript:history.go(-1)">Назад</a>
</h3>

<h2>@ViewBag.Title</h2>

<table>
    <tr>
        <th>Номер тарифа</th>
        <th>Значение</th>
    </tr>

    @{
        foreach (Reading r in (IEnumerable<Reading>)ViewData["Readings"])
        {
            <tr>
                <td>@r.TariffNumber</td>
                <td>@r.Value</td>
            </tr>
        }
    }
</table>

